<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="walterWhite">
    <sequence name="walk-right"       start="0" end="3" loop="false" />
    <sequence name="walk-left"        start="8" end="11" loop="false" />
    <sequence name="walk-up"          start="4" end="7" loop="false" />
    <sequence name="walk-down"        start="12" end="15" loop="false" />
</animation>
